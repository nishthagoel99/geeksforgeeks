#include<stdio.h>
#include<conio.h>
#define size 5

int s[100],top=-1;

void push(int x){
    if(size==top-1){
        printf("stack full");
        return;
    }
    s[++top]=x;
}

void pop(){
    if(top==-1){
        printf("stack empty\n");
        return;
    }
    printf("%d is popped\n",s[top--]);
}

void display(){
    int i;
    if(top==-1){
        printf("stack empty\n");
        return;
    }
    for(i=0;i<=top;i++){
        printf("%d\n",s[i]);
    }
}

void main() {
    display();
    push(1);
    push(2);
    pop();
    push(3);
    push(4);
    display();
}
